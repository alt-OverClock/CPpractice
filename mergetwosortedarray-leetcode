//merge two sorted arrays without taking extra space 
import java.util.*;
class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int q = m+n;
        int j =0 ; 
        for(int i =m ; i < q; i++)
        {
            
            nums1[i]=nums2[j] ;
            System.out.println(nums1[i]);
            j++;
        }
        Arrays.sort(nums1);
        
    }
}
